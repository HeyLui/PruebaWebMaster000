@model IEnumerable<PruebaWebMaster000.Models.Calificacion>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Calificacion1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Comentario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdRestauranteNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Calificacion1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comentario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdRestauranteNavigation.IdRestaurante)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdVotos">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdVotos">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdVotos">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
